<%= form_with(url: podcast_metrics_path(podcast: podcast, date_start: date_start, date_end: date_end, interval: interval), method: :get, id: "metrics") do |form| %>
  <div class="form-floating">
    <%= form.select :selection, uniques_selection_options, {selected: uniques_selection}, data: {action: "apex-nav#updateUniques", apex_nav_path_param: path}, class: "form-select form-select-sm" %>
    <%= form.label :selection %>
  </div>
<% end %>
