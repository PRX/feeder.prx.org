<div class="col-lg-12 my-4">
  <%= turbo_frame_tag "import-table" do %>
    <table class="table table-striped shadow rounded">
      <tbody>
        <tr>
          <td>Import ID</td>
          <td><%= import.id %></td>
        </tr>
        <tr>
          <td>Url</td>
          <td><%= import.url %></td>
        </tr>
        <tr>
          <td>Status</td>
          <td><%= import.status %></td>
        </tr>
        <tr>
          <td>Entries in RSS Feed</td>
          <td><%= import.feed_episode_count if import.feed_episode_count.present? %></td>
        </tr>
        <tr>
          <td>Successfully Imported</td>
          <td><%= import.episode_imports.complete.count %></td>
        </tr>
        <tr>
          <td>In Progress</td>
          <td><%= import.episode_imports.in_progress.count %></td>
        </tr>
        <tr>
          <td>Remaining to Import</td>
          <td><%= import.remaining_import_count %></td>
        </tr>
        <tr>
          <td>Failed</td>
          <td><%= import.episode_imports.failed.count %></td>
        </tr>
        <tr>
          <td>Duplicate GUID Count</td>
          <td><%= import.episode_import_placeholders.count %></td>
        </tr>

        <% unless import.status_complete? || import.status_failed? %>
          <%= link_to "", request.fullpath, class: "d-none", data: {controller: "polling"} %>
        <% end %>
      </tbody>
    </table>
  <% end %>
</div>
