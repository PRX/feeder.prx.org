<div class="overflow-auto text-nowrap">
  <table class="table table-striped shadow rounded">
    <thead>
      <tr class="table-secondary">
        <th width=0><%= t(".guid") %></th>
        <th><%= t(".episode") %></th>
        <th><%= t(".status") %></th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: "imports/episode_import", collection: episode_imports, cached: true %>
      <% if episode_imports.empty? %>
        <tr>
          <td class="text-center text-muted p-4" colspan="100"><%= t(".no_imports") %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<div class="my-4">
  <%= paginate episode_imports %>
  <%= page_entries_info episode_imports %>
</div>
