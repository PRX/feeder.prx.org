<%= turbo_frame_tag "monthly_downloads" do %>
  <div class="col metrics-chart"
    data-controller="apex-chart"
    data-apex-chart-id-value="monthly-downloads"
    data-apex-chart-downloads-value="<%= downloads.to_json %>"
    data-apex-chart-series-type-value="downloads"
    data-apex-chart-chart-type-value="bar">
    <div data-apex-chart-target="chart"></div>
  </div>
<% end %>
